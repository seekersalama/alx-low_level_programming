#include "main.h"
/**
 * _strchr - check the code
 *@dest:char * input pointer
 *@c:char  input pointer
 * Return: char * 0.
 */

char *_strchr(char *dest, char c);
{
	char *ret;

	return (ret);
}
/**
 * _strlen - check the code
 *@s: char * input pointer
 * Return: Always 0.
 */
int _strlen(char *s)
{
	int length;

	length = 0;
	while (s[length] != '\0')
	{
		length++;
	}
	return (length);
}

