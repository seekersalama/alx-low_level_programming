#include <stdlib.h>
/**
 * _calloc - check the code
 *@nmemb: unsigned int
 *@size: unsigned int
 * Return: void *
 */
void *_calloc(unsigned int nmemb, unsigned int size)
{
	unsigned int i;
	char *pt;

	pt = NULL;
	if (nmemb == 0 || size == 0)
		return (NULL);

	pt = malloc(nmemb * size);
	if (pt != NULL)
	{
		for (i = 0; i <= nmemb * size - 1; i++)
		{
			*(pt+size) = 0;
		}
	}
	else
		return (NULL);
	return ((void *) pt);
}

